---
import Footer from "@components/general/Footer.astro";
import Navbar from "@components/general/Navbar.astro";
import MetaHead from "@components/general/MetaHead.astro";
import "../styles/style.css";

const { date, description, image, url, alt, tags } = Astro.props;
const title = import.meta.env.SITE_TITLE
// const headTitle = import.meta.env.SITE_TITLE
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <MetaHead
      title={title}
      description={description}
    />
  </head>

  <body>
    <Navbar />
    <main class="container">
      <slot />
    </main>
  </body>
  <Footer />
</html>